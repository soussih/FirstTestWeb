{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": false,
        "es6": true
    },
    "rules": {
        "comma-dangle" : [
            "error", "never"
        ],
        "no-console": "off",
        "linebreak-style": ["error", "windows"]
    }
}